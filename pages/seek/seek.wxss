  margin-top: 25rpx;
  text-align: center;
}
.money-discounts {
  line-height: 35rpx;
  margin-right: 20rpx;
  padding: 0 10rpx;
  border: 1rpx solid #FF0000;
  border-radius: 10rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
}



